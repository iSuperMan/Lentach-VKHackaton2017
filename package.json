    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-api-middleware": "^2.0.0-beta.3",
    "redux-form": "^7.1.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  }
